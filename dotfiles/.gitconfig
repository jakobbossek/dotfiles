[user]
	name = Jakob Bossek
	email = info@jakobbossek.de
[core]
	editor = subl
	excludesfile = ~/.gitignore_global
[color]
	ui = true
[alias]
	co = checkout
	ci = commit
	st = status
	b  = branch
	lg = log --pretty=format:"%C(yellow)%h%Creset | %s%d [%C(red)%an%Creset@%ad]" --date=short
	
[push]
	default = current
